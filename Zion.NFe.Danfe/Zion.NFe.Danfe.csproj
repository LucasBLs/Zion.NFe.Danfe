    <Authors>Alexandre Laranjeiras</Authors>
    <PackageId>ImprimirDanfeL</PackageId>
    <Version>1.0.1</Version>
    <License>MIT</License>
    <PackageDescription>Gerar nota fiscal em pdf.</PackageDescription>
